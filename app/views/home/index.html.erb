<div class="row">
  <div class="col-sm-4">
    <div class="panel panel-default">
      <div class="panel-body homepanel">
        <h3 class="paneltitle">Stats/readings</h3>
        <ul class="list-group">
          <li class="list-group-item">Last updated: </li>
          <li class="list-group-item">Internal Temp: </li>
          <li class="list-group-item">Humidity: </li>
          <li class="list-group-item">External Temp: </li>
          <li class="list-group-item">Barometer: </li>
          <li class="list-group-item">Gyro: </li>
          <li class="list-group-item">GPS: </li>
        </ul>
      </div>
    </div>
  </div>
  <div class="col-sm-8">
    <div class="panel panel-default">
      <div class="panel-body homepanel">
        <h3 class="paneltitle">Live feed</h3>
        <%= image_tag @latestimage %>
      </div>
    </div>
  </div>
</div>